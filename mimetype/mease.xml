  <mime-type type="application/mass-effect-andromeda-save">
    <comment>Mass Effect: Andromeda save file</comment>
    <comment xml:lang="vi">T&#x1EC7;p l&#x1B0;u Mass Effect: Andromeda</comment>
    <glob pattern="Career*-*"/>
    <magic>
      <match type="string" offset="0" value="FBCHUNKS">
        <match type="little16" offset="8" value="1">
          <match type="string" offset="22" value="FBHEADER">
            <match type="little16" offset="30" value="1"/>
          </match>
        </match>
      </match>
      <match type="string" offset="0" value="SKNUHCBF">
        <match type="big16" offset="8" value="1">
          <match type="string" offset="22" value="REDAEHBF">
            <match type="big16" offset="30" value="1"/>
          </match>
        </match>
      </match>
    </magic>
  </mime-type>
</mime-info>
